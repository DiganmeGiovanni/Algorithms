    "version": "0.2.0",
    "configurations": [
        {
            "name": "Jest sorting",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/jest/bin/jest",
            "args": [
                "--no-cache",
                "${workspaceFolder}/src/sorting/"
            ],
            "disableOptimisticBPs": true
        },
        {
            "name": "Jest arrays",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/jest/bin/jest",
            "args": [
                "--no-cache",
                "${workspaceFolder}/src/arrays/"
            ],
            "disableOptimisticBPs": true
        },
        {
            "name": "Jest tree serialization",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/jest/bin/jest",
            "args": [
                "--no-cache",
                "${workspaceFolder}/src/structures/tree/SerializedTree"
            ],
            "disableOptimisticBPs": true
        }
    ]
}